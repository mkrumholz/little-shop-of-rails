<% content_for :admin do %>
  <%= render partial: './partials/admin_nav' %>
<% end %>

<%= form_with model: [:admin, @merchant], local: true do |f| %>
  <%= f.label :name, "Merchant Name: " %>
  <%= f.text_field :name  %>
  <%= f.label :password, 'Password:' %>
  <%= f.password_field :password %>
  <br><br>
  <%= f.submit %>
<% end %>
